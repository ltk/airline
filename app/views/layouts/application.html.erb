<!DOCTYPE html>
<html>
<head>
  <title>Airline</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% flash.each do |key, value| %>
  <div class="row flash <%= key %>">
    <div class="contain">
      <div class="box">
        <%= value %>
      </div>
    </div>
  </div>
<% end %>
<%= yield %>

</body>
</html>
